{
  "navigationFallback": {
    "rewrite": "/404.html"
  },
  "routes": [
    {
      "route": "/api/projects",
      "methods": ["POST"],
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/projects/*",
      "methods": ["PATCH"],
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/projects/*/upload-image-token",
      "methods": ["POST"],
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "allowedRoles": ["anonymous", "authenticated"],
      "statusCode": 404
    }
  ],
  "build": {
    "appLocation": ".",
    "appBuildCommand": "echo 'Pre-built output in out/ directory'",
    "outputLocation": "out"
  }
}
