{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/*.{css,scss,js,png,gif,ico,jpg,svg,woff,woff2,ttf,eot}"]
  },
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "build": {
    "appLocation": ".",
    "appBuildCommand": "echo 'Pre-built output in out/ directory'",
    "outputLocation": "out"
  }
}
