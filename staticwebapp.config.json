{
  "navigationFallback": {
    "rewrite": "/404.html"
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous", "authenticated"],
      "statusCode": 404
    }
  ],
  "build": {
    "appLocation": ".",
    "appBuildCommand": "echo 'Pre-built output in out/ directory'",
    "outputLocation": "out"
  }
}

